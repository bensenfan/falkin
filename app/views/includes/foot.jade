script.
  //Function to serialize a form if we need to do ajax post on a form
  $.fn.serializeObject = function()
  {// got this function from http://stackoverflow.com/questions/1184624/convert-form-data-to-js-object-with-jquery
      var o = {};
      var a = this.serializeArray();
      $.each(a, function() {
          if (o[this.name] !== undefined) {
              if (!o[this.name].push) {
                  o[this.name] = [o[this.name]];
              }
              o[this.name].push(this.value || '');
          } else {
              o[this.name] = this.value || '';
          }
      });
      return o;
  };